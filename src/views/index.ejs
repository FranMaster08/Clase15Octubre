<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Alumnos</h1>
    <form action="/" method="post">
         <input name="name" placeholder="Nombre" type="text"> 
         <input name="last_name" placeholder="Apellido" type="text"> 
         <input name="age" placeholder="Edad" type="text"> 
         <input name="curso" placeholder="Curso" type="text"> 
         <input value="Crear" placeholder="nombre" type="submit"> 
    </form>
    <ul>
        <% alumnos.forEach(function(alumno){ %>
          <%- include('partials/list', {alumno: alumno}) %>
        <% }); %>
      </ul>
</body>
</html>